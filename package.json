{
  "name": "@53able/conflux",
  "version": "0.1.8",
  "description": "Multi-agent thinking methodology MCP tools for strategic analysis and decision-making",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./mcp-server": {
      "types": "./dist/mcp-server.d.ts",
      "import": "./dist/mcp-server.js"
    }
  },
  "bin": {
    "conflux": "dist/cli/index.js"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && chmod +x dist/cli/index.js dist/mcp/server.js",
    "dev": "tsx --watch src/cli/index.ts",
    "start": "tsx src/cli/index.ts",
    "mcp-server": "tsx src/mcp/server.ts",
    "test": "vitest",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "prepack": "pnpm run build",
    "clean": "rm -rf dist node_modules/.cache"
  },
  "packageManager": "pnpm@9.0.0",
  "keywords": [
    "thinking",
    "methodology",
    "multi-agent",
    "mcp",
    "cli",
    "typescript",
    "ai",
    "reasoning",
    "critical-thinking",
    "logical-thinking"
  ],
  "author": "Conflux",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.15",
    "@ai-sdk/google": "^2.0.13",
    "@ai-sdk/openai": "^2.0.27",
    "@ai-sdk/provider": "^2.0.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "ai": "^5.0.0",
    "chalk": "^5.3.0",
    "commander": "^14.0.0",
    "dotenv": "^17.2.2",
    "ora": "^8.0.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/node": "^22.18.1",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "tsx": "^4.20.0",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.43.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/53able/conflux.git"
  }
}
